<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href="/css/index.css">
</head>

<body>
 
    <div class="container">
        <header>
            公司列表
        </header>
        <main>
            <div class="list">
                <% list.forEach(item=>{ %>

                    <div class="item">
                            <div class="title">
                                <span>抬头</span>
                                <h1><%=item.company%></h1>
                            </div>
                            <div class="one">
                                <span>税号</span>
                                <span><%=item.shuihao%></span>
                            </div>
                            <div class="one">
                                <span>地址</span>
                                <span><%=item.address%></span>
                            </div>
                            <div class="one">
                                <span>电话</span>
                                <span><%=item.mobile%></span>
                            </div>
                            <div class="one">
                                <span>开户行</span>
                                <span><%=item.bank_name%></span>
                            </div>
                            <div class="one">
                                <span>开户账号</span>
                                <span><%=item.bank_card%></span>
                            </div>
                            <div class="one">
                                <span>备注</span>
                                <span><%=item.content%></span>
                            </div>
                        </div>
                <% }) %>
               
            </div>
        </main>
        <footer>
            <a href="/" class="active"> <span>公司列表</span></a>
            <a href="/add.html"><span>新增公司</span></a>
            <a href="/my.html"><span>个人中心</span></a>
        </footer>
    </div>
    
    <script src="/js/axios.js"></script>
    <script src="/js/list.js"></script>
    <script>
        
    new List()
    </script>
</body>

</html>